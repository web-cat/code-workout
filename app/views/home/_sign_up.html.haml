%p.lead Sign Up
.list-group
  = link_to user_facebook_omniauth_authorize_path,
  class: 'btn btn-social btn-facebook list-group-item' do
    %i.fa.fa-facebook
    Sign up with Facebook
  = link_to user_google_oauth2_omniauth_authorize_path,
  class: 'btn btn-social btn-google-plus list-group-item' do
    %i.fa.fa-google
    Sign up with Google
  = link_to user_cas_omniauth_authorize_path,
  class: 'btn btn-social btn-vt list-group-item' do
    %span VT
    Sign up with Virginia Tech CAS
  %div.list-group-item
    .yamm-content
      = render '/devise/registrations/new'
